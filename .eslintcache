ransform = \"translateX(2%) translateY(150%)\"\r\n\t\t} else {\r\n\t\t\t//e.target.previousElementSibling.style.transform = \"translateX(0) translateY(0)\"\r\n\t\t}\r\n\t}\r\n\r\n\treturn (\r\n\t\t<>\r\n\t\t\t<AppSearch></AppSearch>\r\n\t\t\t<Wrapper>\r\n\t\t\t\t<Sidebar\r\n\t\t\t\t\titems={[\r\n\t\t\t\t\t\t{ label: 'Home', link: '/' },\r\n\t\t\t\t\t\t{ label: 'Jobs', isFocus: true, link: '/jobs' },\r\n\t\t\t\t\t\t{ label: 'Earnings', link: 'earnings' },\r\n\t\t\t\t\t\t{ label: 'Settings', link: '' },\r\n\t\t\t\t\t]}></Sidebar>\r\n\t\t\t\t<Content>\r\n\t\t\t\t\t<Form onSubmit={onSubmit}>\r\n\t\t\t\t\t\t<Section>\r\n\t\t\t\t\t\t\t<label>1. Offer Title</label>\r\n\t\t\t\t\t\t\t<input\r\n\t\t\t\t\t\t\t\tname='title'\r\n\t\t\t\t\t\t\t\tref={register({\r\n\t\t\t\t\t\t\t\t\trequired: {\r\n\t\t\t\t\t\t\t\t\t\tvalue: true,\r\n\t\t\t\t\t\t\t\t\t\tmessage: 'Title is required',\r\n\t\t\t\t\t\t\t\t\t},\r\n\t\t\t\t\t\t\t\t\tmaxLength: { value: 100, message: 'Your title is too long!' },\r\n\t\t\t\t\t\t\t\t\tminLength: {\r\n\t\t\t\t\t\t\t\t\t\tvalue: 5,\r\n\t\t\t\t\t\t\t\t\t\tmessage: 'Your title is too short!',\r\n\t\t\t\t\t\t\t\t\t},\r\n\t\t\t\t\t\t\t\t})}\r\n\t\t\t\t\t\t\t\tonBlur={animateLabel}\r\n\t\t\t\t\t\t\t\tonFocus={animateLabel}\r\n\t\t\t\t\t\t\t\tplaceholder={\"Set your offer's title...\"}\r\n\t\t\t\t\t\t\t\tautoComplete={'off'}\r\n\t\t\t\t\t\t\t/>\r\n\t\t\t\t\t\t\t<ErrorMessage>{errors.title?.message}</ErrorMessage>\r\n\t\t\t\t\t\t</Section>\r\n\t\t\t\t\t\t<Section className={'keywords'}>\r\n\t\t\t\t\t\t\t<label>2. Keywords</label>\r\n\t\t\t\t\t\t\tSpecify keywords to help our search engine position your offer\r\n\t\t\t\t\t\t\t<TaggedInput tags={tags} setTags={setTags}></TaggedInput>\r\n\t\t\t\t\t\t\t<ErrorMessage> {customError.tags && 'Set at least one tag!'}</ErrorMessage>\r\n\t\t\t\t\t\t</Section>\r\n\t\t\t\t\t\t<Section className={'fileType'}>\r\n\t\t\t\t\t\t\t<label>3. What are you looking for?</label>\r\n\t\t\t\t\t\t\tData type\r\n\t\t\t\t\t\t\t<select\r\n\t\t\t