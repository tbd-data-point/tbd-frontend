t\tmargin-left: 20px;\n\t\tfont-weight: 100;\n\t}\n`\n\nconst Button = styled.button<ButtonStyled>`\n\tmargin-left: 35px;\n\tfont-size: 15px;\n\twidth: 110px;\n\theight: 35px;\n\tborder: 1px solid black;\n\tborder-radius: 1px;\n\tbox-shadow: 0px 1px 4px 0.1px gray;\n\ttransition: 0.1s all;\n\t&:hover {\n\t\ttransform: scale(1.005);\n\t\tcursor: pointer;\n\t}\n\t${(props) =>\n\t\tprops.upload &&\n\t\t`\n            background-color: #04383b;\n            color: whitesmoke;\n        `};\n\n\t&:focus {\n\t\toutline: none;\n\t}\n`\ntype Section = {\n\tname?: string\n}\n\ninterface ButtonStyled {\n\tupload?: boolean\n}\n\ntype FormData = {\n\ttitle: string\n\tdescription: string\n\tfiles: any\n\tfileType: string\n\tfileCount: number\n\tkeywords: string[]\n}\n\ntype CustomError = {\n\tfiles: number | null | string\n\ttags: number | null | string\n}\n\nconst UploadOffer = () => {\n\tconst [selectedFiles, setSelectedFiles] = useState<any>([])\n\tconst [preview, setPreview] = useState<any>({ index: null, src: null })\n\n\tconst { register, setValue, handleSubmit, errors } = useForm<FormData>()\n\tconst [customError, setCustomError] = useState<CustomError>({ files: null, tags: null })\n\tconst [tags, setTags] = useState<string[]>([])\n\tconst [edit, setEdit] = useState<boolean>(false)\n\n\tinterface Params {\n\t\tdata: string\n\t}\n\tconst data = JSON.parse(atob(useParams<Params>().data))\n\tconst history = useHistory()\n\n\tuseEffect(() => {\n\t\tif (data) {\n\t\t\tObject.keys(data).forEach((key, i) => {\n\t\t\t\tsetValue(key, data[key])\n\t\t\t})\n\t\t\tsetEdit(true)\n\t\t\tsetTags(data.keywords || ['ERROR WHILE FETCHING THE OFFER'])\n\t\t\tPromise.all(\n\t\t\t\tdata.imgLink.map(async (url: string) => {\n\t\t\t\t\treturn axios.get('http://localhost:5000/' + url, { responseType: 'blob' }).then((res) => res.data)\n\t\t\t\t})\n\t\t\t)\n\t\t\t\t.then((files) => {\n\t\t\t\t\tsetSelectedFiles(files)\n\t\t\t\t})\n\t\t\t\t.catch((err) => {\n\t\t\t\t\tsetSelectedFiles([])\n\t\t\t\t\